{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "iamRoute": {
        "ClusterId": "iamCluster",
        "Match": {
          "Path": "/iam/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/iam" }
        ]
      }
    },
    "Clusters": {
      "iamCluster": {
        "Destinations": {
          "iam1": {
            "Address": "http://localhost:5209/"
          }
        }
      }
    }
  }
}
